# service file for maul
# /etc/systemd/system/mock-logger.service
[Unit]
Description=Mock Service
Documentation=https://github.com/pahoughton/agate
After=network-online.target

[Service]
User={{ mon_user }}
ExecStart={{ mon_dir }}/bin/mock-service \
	     --listen-addr ":{{ port_mock_svc }}"

[Install]
WantedBy=multi-user.target
